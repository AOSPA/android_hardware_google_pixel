on boot
    wait /sys/class/leds/vibrator_dual/device

    mkdir /mnt/vendor/persist/haptics 0770 system system
    chmod 770 /mnt/vendor/persist/haptics
    chmod 440 /mnt/vendor/persist/haptics/cs40l25a_dual.cal
    chown system system /mnt/vendor/persist/haptics
    chown system system /mnt/vendor/persist/haptics/cs40l25a_dual.cal

    chown system system /sys/class/leds/vibrator_dual/activate
    chown system system /sys/class/leds/vibrator_dual/brightness
    chown system system /sys/class/leds/vibrator_dual/duration
    chown system system /sys/class/leds/vibrator_dual/state

    chown system system /sys/class/leds/vibrator_dual/device/asp_enable
    chown system system /sys/class/leds/vibrator_dual/device/clab_enable
    chown system system /sys/class/leds/vibrator_dual/device/comp_enable
    chown system system /sys/class/leds/vibrator_dual/device/cp_dig_scale
    chown system system /sys/class/leds/vibrator_dual/device/cp_trigger_duration
    chown system system /sys/class/leds/vibrator_dual/device/cp_trigger_index
    chown system system /sys/class/leds/vibrator_dual/device/cp_trigger_q_sub
    chown system system /sys/class/leds/vibrator_dual/device/cp_trigger_queue
    chown system system /sys/class/leds/vibrator_dual/device/dig_scale
    chown system system /sys/class/leds/vibrator_dual/device/exc_enable
    chown system system /sys/class/leds/vibrator_dual/device/f0_stored
    chown system system /sys/class/leds/vibrator_dual/device/f0_offset
    chown system system /sys/class/leds/vibrator_dual/device/fw_rev
    chown system system /sys/class/leds/vibrator_dual/device/gpio1_enable
    chown system system /sys/class/leds/vibrator_dual/device/gpio1_fall_dig_scale
    chown system system /sys/class/leds/vibrator_dual/device/gpio1_fall_index
    chown system system /sys/class/leds/vibrator_dual/device/gpio1_rise_dig_scale
    chown system system /sys/class/leds/vibrator_dual/device/gpio1_rise_index
    chown system system /sys/class/leds/vibrator_dual/device/gpio_event
    chown system system /sys/class/leds/vibrator_dual/device/gpio_trigger
    chown system system /sys/class/leds/vibrator_dual/device/heartbeat
    chown system system /sys/class/leds/vibrator_dual/device/hw_reset
    chown system system /sys/class/leds/vibrator_dual/device/num_waves
    chown system system /sys/class/leds/vibrator_dual/device/q_stored
    chown system system /sys/class/leds/vibrator_dual/device/redc_comp_enable
    chown system system /sys/class/leds/vibrator_dual/device/redc_stored
    chown system system /sys/class/leds/vibrator_dual/device/standby_timeout
    chown system system /sys/class/leds/vibrator_dual/device/vbatt_max
    chown system system /sys/class/leds/vibrator_dual/device/vbatt_min
    chown system system /sys/class/leds/vibrator_dual/device/vibe_state

    enable vendor.vibrator.cs40l25-dual

service vendor.vibrator.cs40l25-dual /vendor/bin/hw/android.hardware.vibrator-service.cs40l25-dual
    class hal
    user system
    group system

    setenv PROPERTY_PREFIX ro.vendor.vibrator.hal.
    setenv CALIBRATION_FILEPATH /mnt/vendor/persist/haptics/cs40l25a_dual.cal

    setenv HWAPI_PATH_PREFIX /sys/class/leds/vibrator_dual/
    setenv HWAPI_DEBUG_PATHS "
        device/asp_enable
	device/clab_enable
        device/f0_stored
	device/f0_offset
        device/fw_rev
        device/gpio1_fall_dig_scale
        device/gpio1_fall_index
        device/gpio1_rise_dig_scale
        device/gpio1_rise_index
        device/heartbeat
        device/num_waves
        device/q_stored
        device/redc_stored
        state
        "

    disabled
